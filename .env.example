# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags
VITE_USE_MOCK_IMAGE=false

# API Endpoints
VITE_API_WEBHOOK_URL=https://your-webhook-url.com/generate-image

# Authentication Settings
VITE_AUTH_COOKIE_SECURE=true
VITE_AUTH_COOKIE_SAME_SITE=lax
VITE_AUTH_COOKIE_DOMAIN=your_domain.com

# Security Settings
VITE_ENABLE_RATE_LIMITING=true
VITE_CORS_ALLOWED_ORIGINS=https://your-app.vercel.app,https://your-domain.com
