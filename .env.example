# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags
NEXT_PUBLIC_USE_MOCK_IMAGE=false
NEXT_PUBLIC_ENABLE_PAYMENT_FLOWS=false

# API Endpoints
NEXT_PUBLIC_API_WEBHOOK_URL=https://your-webhook-url.com/generate-image

# Authentication Settings
NEXT_PUBLIC_AUTH_COOKIE_SECURE=true
NEXT_PUBLIC_AUTH_COOKIE_SAME_SITE=lax
NEXT_PUBLIC_AUTH_COOKIE_DOMAIN=your_domain.com

# Security Settings
NEXT_PUBLIC_ENABLE_RATE_LIMITING=true
NEXT_PUBLIC_CORS_ALLOWED_ORIGINS=https://your-app.vercel.app,https://your-domain.com

# Analytics (Google Tag Manager)
NEXT_PUBLIC_GTM_ID=

# Google OAuth
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
NEXT_PUBLIC_GOOGLE_REDIRECT_URI_PROD=https://your-domain.com/auth/callback
NEXT_PUBLIC_GOOGLE_REDIRECT_URI_TEST=http://localhost:3000/auth/callback
